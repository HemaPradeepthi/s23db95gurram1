extends layout

block content
  h1= title
  

block content
  h1= title
  - var image_names = ['gadgets.jpg', 'laptop.jpg','mobile.jpg','iphone.jpg','ipad.jpg']
  - for (let img = 0; img <= 2; img++)
    - var diffImg = Math.floor(Math.random() * 5)
    - var image = image_names[diffImg]
    img(src=`/images/${image}`, alt=image, width='300', height='300')